<div class="container">
<h1>Login</h1>
<%= form_tag sessions_path do %>
    <%= label_tag "Email" %>
    <%= text_field_tag :email, nil, placeholder: "email" %>
    <%= label_tag "Password" %>
    <%= password_field_tag :password, nil, placeholder: "Password" %>
    <%= submit_tag "Log In"%>
<% end %>

</div>